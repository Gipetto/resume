{
  "env": {
	"browser": true,
	"node": true,
	"es6": true
  },
  "extends": [
	"eslint:recommended",
	"plugin:react/recommended"
  ],
  "parserOptions": {
	"ecmaFeatures": {
	  "experimentalObjectRestSpread": true,
	  "jsx": true,
	  "modules": true
	},
	"sourceType": "module"
  },
  "plugins": [
	"react"
  ],
  "settings": {
	"react": {
	  "createClass": "createClass",
	  "pragma": "React",
	  "version": "15.4.1"
	}
  },
  "rules": {
	"indent": [
	  "error",
	  2
	],
	"linebreak-style": [
	  2,
	  "unix"
	],
	"quotes": [
	  2,
	  "single"
	],
	"semi": [
	  2,
	  "always"
	],
	"strict": [
	  2,
	  "never"
	],
	"react/jsx-uses-react": 2,
	"react/jsx-uses-vars": 2,
	"react/react-in-jsx-scope": 2
  }
}
