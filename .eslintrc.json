{
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:svelte/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": true,
			"modules": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"settings": {},
	"rules": {
		"indent": [
			"error",
			2
		],
		"eqeqeq": [
			2,
			"smart"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"quotes": [
			2,
			"double"
		],
		"semi": [
			2,
			"never"
		],
		"strict": [
			2,
			"never"
		]
	}
}